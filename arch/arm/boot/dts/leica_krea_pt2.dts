/dts-v1/;
#include "leica_socfpga_arria10.dtsi"

&mmc {
	status = "okay";
	num-slots = <1>;
	broken-cd;
	bus-width = <8>;
};

/ {
	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		fpga_reserved_block {
			compatible = "leica,ion";
			size = <0x60000000>;
		};
	};

	aliases {
		krea_gpio = &gpio1;
	};

	/* i2c for KVIS 0 and 1 */
	i2c5: leicaI2cKVIS@01 {
		#address-cells = <1>;
		#size-cells = <0>;

		compatible = "opencores,i2c-ocores";
		status = "okay";
		reg = <0xFF210000 0x000001F>;

		reg-shift = <2>;
		reg-io-width = <1>;

		interrupts = <0 19 1>;
		interrupt-parent = <&intc>;

		opencores,ip-clock-frequency = <100000000>;
	};

	/* i2c for KVIS 2, 3 and 4 */
	i2c6: leicaI2cKVIS@234 {
		#address-cells = <1>;
		#size-cells = <0>;

		compatible = "opencores,i2c-ocores";
		status = "okay";
		reg = <0xFF210020 0x000001F>;

		reg-shift = <2>;
		reg-io-width = <1>;

		interrupts = <0 20 1>;
		interrupt-parent = <&intc>;

		opencores,ip-clock-frequency = <100000000>;
	};

	/* i2c for KELA and KAZA */
	i2c7: leicaI2cKelaKaza@0 {
		#address-cells = <1>;
		#size-cells = <0>;

		compatible = "opencores,i2c-ocores";
		status = "okay";
		reg = <0xFF210040 0x000001F>;

		reg-shift = <2>;
		reg-io-width = <1>;

		interrupts = <0 21 1>;
		interrupt-parent = <&intc>;

		/* i2c-controller clock */
		opencores,ip-clock-frequency = <100000000>;

		/* i2c-bus clock*/
		clock-frequency = <100000>;
	};

