			compatible = "ALTR,uart-11.1", "altr,uart-1.0";
			reg = < 0xFF210060 0x00000020 >;
			interrupt-parent = < &intc >;
			interrupts = < 0 22 IRQ_TYPE_LEVEL_HIGH >;
			current-speed = < 9600 >;	/* 9600 baud */
			clock-frequency = < 100000000 >;	/* 100Mhz */
	};

	leicaKelaSPI: leicaSpiKELA@ff210200 {
			compatible = "ALTR,spi-11.1", "altr,spi-1.0";
			reg = < 0xFF210200 0x00000020 >;
			interrupt-parent = < &intc >;
			interrupts = < 0 25 1 >;
			#address-cells = < 1 >;
			#size-cells = < 0 >;

			spiADCKELA: spiADCKELA@0 {
					compatible = "leica,spidev";
					spi-max-frequency = < 625000 >;
					reg = < 0x00000000 >;
