&i2c0 {
	status = "okay";
	speed-mode = <0>;

	/****************************************************************/
	/*                      KREA board modules                      */
	/*             (attached before the Level translator)           */
	/****************************************************************/
	/* Temperature sensor close to the FPGA */
	 leicaTempKREA_FPGA0: leicaTempKREA_FPGA@0 {
		compatible = "tmp102";
		reg = <0x48>;
	};

	/* Temperature away from the FPGA  */
	 leicaTempKREA_ENV0: leicaTempKREA_ENV@0 {
		compatible = "tmp102";
		reg = <0x49>; /* 0-series new address would be 0x4A */
	};

	/* 64kbyte EEPROM at KREA */
	leicaMemKREA0: leicaMemKREA@0 {
		compatible = "at24,24c512";
		reg = <0x50>;
		pagesize = <128>;
	};
	/****************************************************************/

	/****************************************************************/
	/*               Kazaar Wiring Board (KAWI)                     */
	/*          (attached after the Level translator)               */
	/****************************************************************/
	/* Temperature Sensor attached to KAWI board */
	leicaTempKAWI0: leicaTempKAWI@0 {
		compatible = "lm75,tmp75";
		reg = <0x4B>;
	};

	/* External Temperature module (KTMP) */
	 leicaTempKAWI_KTMP0: leicaTempKAWI_KTMP@0 {
		compatible = "tmp112";
		reg = <0x4A>;  /* 0-series new address would be 0x49 */
	};

	/* Nadir Lights */
	leicaLedsKAWI_KNAL: leicaLedsKAWI_KNAL@40 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "ti,tlc59108";
		reg = <0x40>;

		/* Note: LED 5 and 7 are not connected */
		fs_red@0	{ label = "fs_red";	reg = <0x0>; };
		fs_green@1  { label = "fs_green";  reg = <0x1>; };
		bs_red@2	{ label = "bs_red";	reg = <0x2>; };
		bs_green@3  { label = "bs_green";  reg = <0x3>; };
		fs_yellow@4 { label = "fs_yellow"; reg = <0x4>; };
		bs_yellow@6 { label = "bs_yellow"; reg = <0x6>; };
	};
	/****************************************************************/
};

&i2c1  {

	speed-mode = <0>;
	status = "okay";
	
	/****************************************************************/
	/*        Kazaar Frame Camera Board 0 (KAFC0)                   */
	/****************************************************************/
	/* 64kbyte EEPROM at KAFC0 (Kazaar Frame Camera Board 0) */
	leicaMemKAFC0: leicaMemKAFC@0 {
		compatible = "at24,24c512";
		reg = <0x50>;
		pagesize = <128>;
	};

	/* Temperature sensor at KAFC0 */
	 leicaTempKAFC0: leicaTempKAFC@0 {
		compatible = "lm75,tmp75";
		reg = <0x48>;
	};
	/****************************************************************/

	/****************************************************************/
	/*        Kazaar Frame Camera Board 1 (KAFC1)                   */
	/****************************************************************/
	/* 64kbyte EEPROM at KAFC1 */
	leicaMemKAFC1: leicaMemKAFC@1 {
		compatible = "at24,24c512";
		reg = <0x52>;
		pagesize = <128>;
	};

	/* Temperature sensor at KAFC1 */
	leicaTempKAFC1: leicaTempKAFC@1 {
		compatible = "lm75,tmp75";
		reg = <0x4a>;
	};
	/****************************************************************/

	/****************************************************************/
	/*        Kazaar Frame Camera Board 2 (KAFC2)                   */
	/****************************************************************/
	/* 64kbyte EEPROM at KAFC2 */
	leicaMemKAFC2: leicaMemKAFC@2 {
		compatible = "at24,24c512";
		reg = <0x54>;
		pagesize = <128>;
	};

	/* Temperature sensor at KAFC2 */
	leicaTempKAFC2: leicaTempKAFC@2 {
		compatible = "lm75,tmp75";
		reg = <0x4c>;
	};
	/****************************************************************/

	/****************************************************************/
	/*                Kazaar EDM Board (KEDM)                       */
	/****************************************************************/
	/* 64kbyte EEPROM at KEDM */
	leicaMemKEDM0: leicaMemKEDM@0 {
		compatible = "at24,24c512";
		reg = <0x57>;
		pagesize = <128>;
	};

	/* Temperature sensor at KEDM */
	 leicaTempKEDM0: leicaTempKEDM@0 {
		compatible = "lm75,tmp75";
		reg = <0x4f>;
	};
	
	/* External Temperature module (KTMP) */
	 leicaTempKEDM_KTMP0: leicaTempKEDM_KTMP@0 {
		compatible = "tmp112";
		reg = <0x49>;
	};
	/****************************************************************/
};

/* node for SPI0 bus. SPI0 is defined in socfpga
 * here you add HW that is connected to SPI0
*/

&spi0 {
	status = "okay";
	/* This is a kind of template how to add spi devices
	 * that can be accessed from userspace
	*/
	stepperSPI: spiStepperKADR@0 {
		compatible = "leica,spidev";
		reg = <0>; /*chip select 0 */
		spi-max-frequency = <2000000>;
		enable-dma = <1>;
	};

	bldcSPI: spiBLDCKADR@0 {
		compatible = "leica,spidev";
		reg = <1>; /*chip select 1 */
		spi-max-frequency = <10000000>;
		enable-dma = <1>;
	};
};

/* node for SPI1 bus. SPI1 is defined in socfpga
 * here you add HW that is connected to SPI1
*/
&spi1 {
	status = "okay";
	kedmADCSPI: spiADCKEDM@0 {
		compatible = "leica,spidev";
		reg = <0>; /*chip select 0 */
		spi-max-frequency = <10000000>;
		enable-dma = <1>;
	};
	kedmMonitorSPI: spiMonitorKEDM@0 {
		compatible = "leica,spidev";
		reg = <1>; /*chip select 1 */
		spi-max-frequency = <10000000>;
		enable-dma = <1>;
	};
