		compatible = "altr,quadspi-1.0";
		reg = <0xc0040000 0x20>, <0xc4000000 0x04000000>;
		reg-names = "avl_csr", "avl_mem";
		#address-cells = <1>;
		#size-cells = <0>;
		flash0: epcq512@0 {
			compatible = "altr,epcq512";
			#address-cells = <1>;
			#size-cells = <1>;
			/* QSPI on KREA has 512MB, but only 64MB are mapped
			   in FPGA */
			partition@0 {
				label = "FPGA image";
				reg = <0x0 0x04000000>;
			};
		};
	};

